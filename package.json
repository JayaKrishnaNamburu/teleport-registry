{
  "name": "teleport-registry",
  "description": "A regsitry to save all components that are designed in playground",
  "version": "0.0.0",
  "main": "lib/index.js",
  "types": "lib/types.d.ts",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -p ./",
    "dev": "ts-node-dev ./src/index.ts",
